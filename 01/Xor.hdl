/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in= a, out= A);
    Not(in= b, out= B);
    And(a= A, b= b, out= E);
    And(a= B, b= a, out= F);
    Or(a= E, b= F, out= out);
	
    
}